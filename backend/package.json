{
  "name": "event-management-system",
  "version": "1.0.3",
  "description": "Sistem Informasi Manajemen Kegiatan (Event Management System)",
  "main": "src/app.js",
  "scripts": {
    "start": "NODE_OPTIONS='--no-warnings --max-old-space-size=4096' node src/app.js",
    "dev": "nodemon src/app.js",
    "postinstall": "npx prisma generate",
    "build": "npx prisma migrate deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "node src/utils/seed.js",
    "db:studio": "npx prisma studio",
    "db:health": "node scripts/database-health-check.js",
    "db:backup": "bash scripts/backup-database.sh",
    "db:restore": "node scripts/restore-data.js",
    "monitoring:setup": "node scripts/setup-monitoring.js",
    "monitoring:start": "docker compose -f docker-compose.monitoring.yml up -d",
    "monitoring:stop": "docker compose -f docker-compose.monitoring.yml down",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "create-admin": "node scripts/create-admin.js",
    "create-dummy-events": "node scripts/create-dummy-events.js",
    "migrate:customer-service": "node scripts/migrate-enums-direct.js",
    "migrate:customer-service:sql": "psql $DATABASE_URL -f scripts/migrate-enums-direct.sql",
    "fix:tickets": "node scripts/fix-multiple-tickets-production.js",
    "fix:db": "node scripts/fix-production-database.js && node scripts/fix-multiple-tickets-production.js",
    "fix:all": "npm run fix:db",
    "check:local": "node scripts/check-local-database.js",
    "check:railway": "node scripts/check-railway-database.js"
  },
  "keywords": [
    "event-management",
    "nodejs",
    "express",
    "postgresql",
    "redis",
    "jwt"
  ],
  "author": "Senior Software Engineer",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.517.0",
    "@getbrevo/brevo": "^3.0.1",
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^10.12.0",
    "@sentry/profiling-node": "^10.12.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.7",
    "dotenv": "^17.2.3",
    "duitku-nodejs": "^0.0.8",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.2.1",
    "express-slow-down": "^3.0.1",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "joi": "^18.0.2",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.3",
    "mime-types": "^3.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "pdfkit": "^0.17.2",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.19.0",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "sharp": "^0.34.3",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "ws": "^8.18.3",
    "xendit-node": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.22.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
